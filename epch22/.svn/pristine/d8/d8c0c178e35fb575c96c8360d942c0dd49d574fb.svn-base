<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:drawable="@drawable/homeexpo_disabled" 
        android:state_enabled="false"></item>						<!-- disabled button -->
    <item android:drawable="@drawable/homeexpo_onclick" 
        android:state_pressed="true"/>					<!-- press -->
    <item android:state_focused="true"
           android:drawable="@drawable/homeexpo_onclick" /><!-- focus -->
    <item android:state_hovered="true"
         android:drawable="@drawable/homeexpo_onclick" ></item>       
    <item android:drawable="@drawable/homeexpo"/>				<!-- default -->

</selector>
